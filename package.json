{
    "name": "separate-number",
    "displayName": "separate-number",
    "description": "Toggle number in separated/unseparated mode",
    "version": "0.0.4",
    "publisher": "maoguangming",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:separate-number.toggle"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "separate-number.toggle",
                "title": "Separate Number: Toggle"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    },
    "icon": "icon.png"
}